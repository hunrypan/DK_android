<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/page3imgv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/screen5"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/page3BT"
        android:layout_width="120dp"
        android:layout_height="84dp"
        android:layout_marginStart="120dp"
        android:layout_marginBottom="10dp"
        android:alpha="0"
        android:background="@drawable/outwater"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/page3BT_s1"
        android:layout_width="96dp"
        android:layout_height="38dp"
        android:layout_marginStart="27dp"
        android:layout_marginBottom="204dp"
        android:alpha="0"
        android:background="@drawable/roundbt15"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/page3BT_s2"
        android:layout_width="96dp"
        android:layout_height="38dp"
        android:layout_marginStart="132dp"
        android:layout_marginBottom="204dp"
        android:alpha="0"
        android:background="@drawable/roundbt15"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/page3BT_s3"
        android:layout_width="96dp"
        android:layout_height="38dp"
        android:layout_marginStart="237dp"
        android:layout_marginBottom="204dp"
        android:alpha="0"
        android:background="@drawable/roundbt15"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/page3imageV1"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="60dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/page3BT_s1"
        app:srcCompat="@drawable/bticon1" />

    <ImageView
        android:id="@+id/page3imageV2"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="160dp"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/page3BT_s2"
        app:srcCompat="@drawable/bticon1" />

    <ImageView
        android:id="@+id/page3imageV3"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="268dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/page3BT_s3"
        app:srcCompat="@drawable/bticon1" />

    <Button
        android:id="@+id/page3BT2"
        android:layout_width="266dp"
        android:layout_height="63dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="103dp"
        android:alpha="0"
        android:background="@drawable/roundbt20"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>